<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
    
  <!-- the basics -->
  <groupId>ch.emf.info</groupId>
  <artifactId>daolayer</artifactId>
  <version>5.34</version>
  <packaging>jar</packaging>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
  </properties>
    
  <!-- additional repository -->  
  <repositories>
    <repository>        
      <id>emfinfo</id>
      <name>emfinfo.github.io</name>
      <url>http://emfinfo.github.io/javalibs/releases</url>
    </repository>
  </repositories>      
    
  <!-- dependencies -->
  <dependencies>

    <!-- dependencies for compile -->
    <dependency>
      <groupId>org.eclipse.persistence</groupId>
      <artifactId>org.eclipse.persistence.jpa</artifactId>
      <version>2.6.2</version>
    </dependency>
    
    <dependency>
      <groupId>javax.ejb</groupId>
      <artifactId>javax.ejb-api</artifactId>
      <version>3.2</version>
    </dependency>
    
    <dependency>
      <groupId>org.slf4j</groupId>
      <artifactId>slf4j-log4j12</artifactId>
      <version>1.7.18</version>
    </dependency>

    <!-- dependencies for tests -->
    <dependency>
      <groupId>org.hamcrest</groupId>
      <artifactId>hamcrest-core</artifactId>
      <version>1.3</version>
      <scope>test</scope>
    </dependency>
    
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.11</version>
      <scope>test</scope>
    </dependency>
    
    <dependency>
      <groupId>org.projectlombok</groupId>
      <artifactId>lombok</artifactId>
      <version>1.16.8</version>
      <scope>test</scope>
    </dependency>
    
    <dependency>       
      <groupId>ch.jcsinfo</groupId>
      <artifactId>basiclib</artifactId>
      <version>1.01</version>
      <scope>test</scope>
    </dependency>    
    
    <dependency>
      <groupId>mysql</groupId>
      <artifactId>mysql-connector-java</artifactId>
      <version>5.1.22</version>
      <scope>test</scope>
    </dependency>
    
  </dependencies>      
    
  <!-- build settings -->
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-source-plugin</artifactId>
        <version>3.0.0</version>        
      </plugin>
      
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>2.10.3</version>   
        <configuration>     
          <tags>
            <tag>
              <name>tagvalue</name>
            </tag>
            <tag>
              <name>composed</name>
            </tag>
            <tag>
              <name>has</name>
            </tag>
            <tag>
              <name>depend</name>
            </tag>
            <tag>
              <name>hidden</name>
            </tag>
            <tag>
              <name>stereotype</name>
            </tag>
            <tag>
              <name>match</name>
            </tag>
            <tag>
              <name>opt</name>
            </tag>
            <tag>
              <name>view</name>
            </tag>
            <tag>
              <name>note</name>
            </tag>
          </tags>                                
        </configuration>        
      </plugin>

      <plugin>
        <artifactId>maven-deploy-plugin</artifactId>
        <version>2.8.2</version>        
      </plugin>
      
      <plugin>
        <artifactId>maven-project-info-reports-plugin</artifactId>
        <version>2.9</version>        
      </plugin>
      
      <plugin>
        <groupId>net.ju-n.maven.plugins</groupId>
        <artifactId>checksum-maven-plugin</artifactId>
        <version>1.3</version>
        <executions>
          <execution>
            <goals>
              <goal>files</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <fileSets>
            <fileSet>
              <directory>target</directory>
              <includes>
                <include>*.pom</include>
                <include>*.jar</include>
              </includes>
            </fileSet>
          </fileSets>                  
        </configuration>
      </plugin>    
    </plugins>
  </build>      
  
  <reporting>                                                                              
    <plugins>                                                                            
      <plugin>                                                                         
        <groupId>org.apache.maven.plugins</groupId>                                  
        <artifactId>maven-javadoc-plugin</artifactId>                                
        <version>2.10.3</version>                                                       
        <configuration>     
          <tags>
            <tag>
              <name>tagvalue</name>
            </tag>
            <tag>
              <name>composed</name>
            </tag>
            <tag>
              <name>has</name>
            </tag>
            <tag>
              <name>depend</name>
            </tag>
            <tag>
              <name>hidden</name>
            </tag>
            <tag>
              <name>stereotype</name>
            </tag>
            <tag>
              <name>match</name>
            </tag>
            <tag>
              <name>opt</name>
            </tag>
            <tag>
              <name>view</name>
            </tag>
            <tag>
              <name>note</name>
            </tag>
          </tags>                                
          <doclet>org.umlgraph.doclet.UmlGraphDoc</doclet>                         
          <docletArtifact>                                                         
            <groupId>org.umlgraph</groupId>                                      
            <artifactId>umlgraph</artifactId>                                    
            <version>5.6</version>                                               
          </docletArtifact>                                                        
          <additionalparam>-quiet</additionalparam>
          <additionalparam>-dotexecutable /usr/local/bin/dot</additionalparam>
          <!--<additionalparam>-horizontal</additionalparam>-->
          <!--          <additionalparam>-all</additionalparam>-->
          <additionalparam>-nodefillcolor gray97</additionalparam>
          <additionalparam>-nodefontsize 9</additionalparam>
          <additionalparam>-nodefontclassname Monaco</additionalparam>
          <additionalparam>-nodefontclassssize 14</additionalparam>
          <additionalparam>-nodesep 1.0</additionalparam>
          <additionalparam>-edgefontsize 48</additionalparam>
          <additionalparam>-edgefontcolor red</additionalparam>
          <additionalparam>-edgecolor forestgreen</additionalparam>
          <!--<additionalparam>-quiet -views -all -horizontal</additionalparam>-->    
          <additionalparam>-quiet -views -all</additionalparam>    
                                                        
          <useStandardDocletOptions>true</useStandardDocletOptions>                
        </configuration>                                                             
      </plugin>                                                                        
    </plugins>                                                                           
  </reporting> 
  
  <!-- more project information -->
  <name>DaoLayer</name>
  <description>A dao layer to use JPA managed objects from a database.</description>
  <licenses>
    <license>
      <name>MIT License</name>
      <url>http://www.opensource.org/licenses/MIT</url>
    </license>
  </licenses>  
  <url>https://github.com/emfinfo/daolayer</url>
  <developers>
    <developer>
      <id>STT</id>
      <name>Jean-Claude Stritt</name>
      <email>strittjc@edufr.ch</email>
      <url>http://jcstritt.emf-informatique.ch/</url>
      <organization>EMF</organization>
      <organizationUrl>http://www.emf.ch</organizationUrl>
      <roles>
        <role>engineer</role>
        <role>developer</role>
      </roles>
      <timezone>Europe/Switzerland</timezone>
      <properties>
        <picUrl>http://jcstritt.emf-informatique.ch/pic/jcs01-square.jpg</picUrl>
      </properties>
    </developer>

    <developer>
      <id>PAM</id>
      <name>Pierre-Alain Mettraux</name>
      <email>mettrauxpa@edufr.ch</email>
      <url>http://www.mettraux.ch//</url>
      <organization>EMF</organization>
      <organizationUrl>http://www.emf.ch</organizationUrl>
      <roles>
        <role>engineer</role>
        <role>developer</role>
      </roles>
      <timezone>Europe/Switzerland</timezone>
      <properties>
        <picUrl>http://jcstritt.emf-informatique.ch/pic/pam01-square.jpg</picUrl>
      </properties>
    </developer>
    
  </developers>
    
</project>